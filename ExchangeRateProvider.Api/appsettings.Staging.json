{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "ExchangeRateProvider": "Information"
    }
  },
  "ExchangeRateProvider": {
    "CnbExchangeRateUrl": "https://api.cnb.cz/cnbapi/exrates/daily",
    "CacheExpirationMinutes": 90,
    "TimeoutSeconds": 45,
    "MaxCurrencies": 30
  },
  "Redis": {
    "Enabled": true,
    "Configuration": "redis-staging:6379",
    "InstanceName": "ExchangeRatesStaging"
  },
  "CnbCacheStrategy": {
    "PublicationWindowMinutes": 7,
    "WeekdayHours": 1.5,
    "WeekendHours": 18
  }
}